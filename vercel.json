{
    "version": 2,
    "builds": [
        {
            "src": "stremio_addon.js",
            "use": "@vercel/node@18"
        },
        {
            "src": "api/criterion_movies.json",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        { "src": "/api/criterion_movies.json", "dest": "/api/criterion_movies.json" },
        { "src": "/(.*)", "dest": "stremio_addon.js" }
    ]
}
